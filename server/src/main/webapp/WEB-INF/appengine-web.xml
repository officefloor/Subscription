<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
	<runtime>java8</runtime>
	<threadsafe>true</threadsafe>
	<warmup-requests-enabled>false</warmup-requests-enabled>
	<automatic-scaling>
		<min-instances>0</min-instances>
		<max-concurrent-requests>50</max-concurrent-requests>
	</automatic-scaling>
	<system-properties>
		<property name="server.domain" value="officefloor.appspot.com" />
		<property name="https.port" value="443" />
	</system-properties>
	<!-- <env-variables> <env-var name="initialise_file_path" value="classpath://config/subscription.prod" /> </env-variables> -->
</appengine-web-app>